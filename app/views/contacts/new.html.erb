<h1>New Contact Form</h1>

<form action="/add_contact" method="POST">
    <label>Name</label>
    <input type='text' name="contact[name]"/>
    <label>Phone</label>
    <input type='text' name="contact[phone]"/>
    <label>Email</label>
    <input type='text' name="contact[email]"/>
    <input type="submit" value="Add Contact"/>
</form>